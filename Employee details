include <stdio.h>
#include <stdlib.h>
 
typedef struct{
 
    char name[30];
    int id;
    int salary;
 
} Employee;
 
int main()
{
    int i, n=20;
 
    Employee employees[n]
        
printf("Enter %d Employee Details\n",n);
    for(i=0; i<n; i++)
{
 printf("Employee %d:- \n",i+1);
        printf("Name:");
        scanf("%s",employees[i].name);
        printf("Id: ");
        scanf("%d",&employees[i].id);
       printf("\n");
    }
 
